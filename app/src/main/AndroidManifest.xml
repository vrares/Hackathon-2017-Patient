<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="org.hackathon">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.CALL_PHONE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.IdentificationActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.ValidationActivity" />
        <activity android:name=".activities.UserDetailsActivity">

        </activity>
        <activity android:name=".activities.AppointmentActivity" />
        <activity android:name=".activities.HistoryActivity" />
        <activity android:name=".activities.AllergiesActivity" />
        <activity android:name=".activities.PrescriptionActivity"></activity>
    </application>

</manifest>